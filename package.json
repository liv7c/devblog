{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production netlify dev",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|mdx)\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ./app",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ./app --fix",
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "feed": "^4.2.2",
    "highlight.js": "^11.6.0",
    "luxon": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-highlight": "^5.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/luxon": "^3.0.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.24.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
