{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production netlify dev",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx|mdx)\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ./app",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext js,jsx,ts,tsx ./app --fix",
    "validate": "lint-staged",
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "prepare": "husky install"
  },
  "dependencies": {
    "@netlify/functions": "1.3.0",
    "@remix-run/netlify": "1.7.5",
    "@remix-run/node": "1.7.5",
    "@remix-run/react": "1.7.5",
    "clsx": "1.2.1",
    "cross-env": "7.0.3",
    "feed": "4.2.2",
    "highlight.js": "11.6.0",
    "luxon": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-highlight": "6.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.5",
    "@remix-run/eslint-config": "1.7.5",
    "@remix-run/serve": "1.7.5",
    "@tailwindcss/typography": "0.5.8",
    "@types/luxon": "3.1.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.27.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
